    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/adapter-pg": "^6.17.1",
    "@prisma/client": "6.17.1",
    "@prisma/engines": "6.17.1",
    "@prisma/instrumentation": "^6.17.1",
    "@unique-ag/aes-gcm-encryption": "workspace:*",
    "@unique-ag/instrumentation": "workspace:*",
    "@unique-ag/logger": "workspace:*",
    "@unique-ag/mcp-oauth": "workspace:*",
    "@unique-ag/mcp-server-module": "workspace:*",
    "@unique-ag/probe": "workspace:*",
    "axios": "^1.11.0",
    "cache-manager": "^7.2.0",
    "dotenv": "^16.5.0",
    "jsonwebtoken": "^9.0.2",
    "nestjs-otel": "^7.0.1",
    "nestjs-pino": "^4.4.0",
    "nestjs-zod": "^5.0.1",
    "openid-client": "^5.7.1",
    "passport": "^0.7.0",
    "passport-strategy": "^1.0.0",
    "pino-http": "^10.5.0",
    "prisma": "^6.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "serialize-error-cjs": "^0.2.0",
    "typeid-js": "^1.2.0",
    "zod": "^4.1.5"
