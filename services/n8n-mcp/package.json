    "@types/passport-strategy": "^0.2.38",
    "@vitest/coverage-istanbul": "^3.2.4",
    "globals": "^16.3.0",
    "pino-http-print": "^3.1.0",
    "source-map-support": "^0.5.21",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4",
    "orval": "^7.11.2"
  }
