{
  "name": "@unique-ag/confluence-connector",
  "description": "Take content from Confluence and send it to Unique AI for RAG ingestion.",
  "version": "2.0.0-alpha.0",
  "author": "",
  "private": true,
  "license": "Unique License v1",
  "files": [
    "dist",
    "deploy"
  ],
  "scripts": {
    "build": "nest build",
    "dev": "nest start",
    "debug": "nest start --debug --watch",
    "db:generate": "echo \"No database for this server\"",
    "db:migrate": "echo \"No database for this server\"",
    "db:push": "echo \"No database for this server\"",
    "db:migrate:deploy": "echo \"No database for this server\"",
    "codegen": "echo \"No codegen needed for this server\"",
    "style": "biome check .",
    "style:fix": "biome check . --write",
    "check-types": "tsc --noEmit",
    "check-all": "pnpm style:fix && pnpm check-types",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --config vitest.coverage.config.ts && istanbul-badges-readme --style='square-flat'"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@proventuslabs/nestjs-zod": "^2.0.0",
    "@unique-ag/instrumentation": "workspace:*",
    "@unique-ag/logger": "workspace:*",
    "@unique-ag/probe": "workspace:*",
    "@unique-ag/unique-api": "workspace:*",
    "bottleneck": "^2.19.5",
    "cron": "^4.3.0",
    "js-yaml": "^4.1.0",
    "nestjs-otel": "^7.0.1",
    "nestjs-pino": "^4.4.0",
    "p-limit": "^2.3.0",
    "pino": "^10.3.1",
    "pino-http": "^10.5.0",
    "reflect-metadata": "^0.2.2",
    "remeda": "^2.32.0",
    "rxjs": "^7.8.2",
    "serialize-error-cjs": "^0.2.0",
    "undici": "^7.18.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@suites/di.nestjs": "^3.0.1",
    "@suites/doubles.vitest": "^3.0.1",
    "@suites/unit": "^3.0.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.14",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "globals": "^16.3.0",
    "istanbul-badges-readme": "^1.9.0",
    "pino-http-print": "^3.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  }
}
