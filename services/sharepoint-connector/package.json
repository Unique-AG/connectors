{
  "name": "@unique-ag/sharepoint-connector",
  "description": "Take content from SharePoint and send it to Unique AI for RAG ingestion..",
  "version": "2.0.0-beta.5",
  "author": "",
  "private": true,
  "license": "Unique License v1",
  "files": [
    "dist",
    "deploy"
  ],
  "scripts": {
    "build": "nest build",
    "dev": "nest start",
    "debug": "nest start --debug --watch",
    "db:generate": "echo \"No database for this server\"",
    "db:migrate": "echo \"No database for this server\"",
    "db:push": "echo \"No database for this server\"",
    "db:migrate:deploy": "echo \"No database for this server\"",
    "codegen": "echo \"No codegen needed for this server\"",
    "style": "biome check .",
    "style:fix": "biome check . --write",
    "check-types": "tsc --noEmit",
    "check-all": "pnpm style:fix && pnpm check-types",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:watch": "vitest watch --config vitest.e2e.config.ts",
    "test:coverage": "vitest run --config vitest.coverage.config.ts && istanbul-badges-readme --style='square-flat'"
  },
  "dependencies": {
    "@azure/identity": "^4.12.0",
