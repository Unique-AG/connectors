    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@proventuslabs/nestjs-zod": "^2.0.0",
    "@unique-ag/instrumentation": "workspace:*",
    "@unique-ag/logger": "workspace:*",
    "@unique-ag/probe": "workspace:*",
    "bottleneck": "^2.19.5",
    "cron": "^4.3.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "nestjs-otel": "^7.0.1",
    "nestjs-pino": "^4.4.0",
    "nestjs-zod": "^5.0.1",
    "p-limit": "^2.3.0",
    "pino-http": "^10.5.0",
    "reflect-metadata": "^0.2.2",
    "remeda": "^2.32.0",
    "rxjs": "^7.8.2",
