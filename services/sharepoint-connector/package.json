    "@vitest/coverage-istanbul": "^3.2.4",
    "globals": "^16.3.0",
    "istanbul-badges-readme": "^1.9.0",
    "pino-http-print": "^3.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  }
