NODE_ENV=development
LOG_LEVEL=debug
PORT=9542
PROCESSING_CONCURRENCY=1
STEP_TIMEOUT_SECONDS=300
MAX_FILE_SIZE_BYTES=209715200
ALLOWED_MIME_TYPES=application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document,application/vnd.openxmlformats-officedocument.wordprocessingml.document

GRAPH_CLIENT_ID=your-graph-client-id
GRAPH_CLIENT_SECRET=your-graph-client-secret
GRAPH_TENANT_ID=your-graph-tenant-id
GRAPH_API_URL=https://graph.microsoft.com

SHAREPOINT_SITES=your-sharepoint-site-id
SHAREPOINT_SYNC_COLUMN_NAME=your-sync-column-name

UNIQUE_FILE_DIFF_BASE_PATH=https://your-environment.unique.app/
UNIQUE_FILE_DIFF_PARTIAL_KEY=sharepoint/default
UNIQUE_API_ENDPOINT=https://api.your-environment.com/graphql
UNIQUE_API_TOKEN_URL=https://zitadel.your-domain.com/oauth/v2/token
UNIQUE_API_CLIENT_ID=your-unique-api-client-id
UNIQUE_API_CLIENT_SECRET=your-unique-api-client-secret

UNIQUE_INGESTION_URL_GRAPHQL=https://gateway.your-environment.com/ingestion-gen2/graphql
UNIQUE_INGESTION_URL=https://gateway.your-environment.com/ingestion-gen2/v1/content
UNIQUE_SCOPE_ID=your-scope-id

ZITADEL_OAUTH_TOKEN_URL=https://id.your-environment.com/oauth/v2/token
ZITADEL_PROJECT_ID=your-zitadel-project-id
ZITADEL_CLIENT_ID=your-zitadel-client-id@your-domain.com
ZITADEL_CLIENT_SECRET=your-zitadel-client-secret

# OpenTelemetry Configuration (for Prometheus metrics)
OTEL_METRICS_EXPORTER=prometheus
OTEL_EXPORTER_PROMETHEUS_HOST=localhost
OTEL_EXPORTER_PROMETHEUS_PORT=8081