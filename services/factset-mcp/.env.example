LOG_LEVEL=debug
SELF_URL=http://localhost:3000
DATABASE_URL=postgresql://postgres:postgres@localhost:9001/factset-mcp
ACCESS_TOKEN_EXPIRES_IN_SECONDS=60 # 1 minute
REFRESH_TOKEN_EXPIRES_IN_SECONDS=2592000 # 30 days
HMAC_SECRET=<hmac-secret> # Random 32-char secret. Create with e.g. openssl rand -hex 32
ENCRYPTION_KEY=<encryption-key> # 32-char hex string. Create with e.g. openssl rand -hex 32
FACTSET_AUTH_CONFIG=<factset-auth-json-base64-encoded>

# Create a new App in a Zitadel project of type Web - Code.
# Ensure the Zitadel App returns a Auth Token Type JWT and includes the User roles in the access token, as well as the ID Token
ZITADEL_CLIENT_ID=
ZITADEL_CLIENT_SECRET=
ZITADEL_ISSUER=http://localhost:10116
ZITADEL_REQUIRED_ROLE=mcp.factset

