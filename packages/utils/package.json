{
  "name": "@unique-ag/utils",
  "description": "Shared utility modules for the connectors monorepo",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./zod": {
      "types": "./src/zod.ts",
      "require": "./dist/zod.js",
      "default": "./dist/zod.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "format": "biome format .",
    "format:fix": "biome format . --write",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "remeda": "^2.32.0",
    "serialize-error-cjs": "^0.2.0"
  },
  "peerDependencies": {
    "typeid-js": "^1.2.0",
    "zod": "^4.1.5"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    },
    "typeid-js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@types/node": "^22.13.14",
    "typeid-js": "^1.2.0",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4",
    "zod": "^4.1.5"
  }
}
