{
  "name": "@unique-ag/unique-api",
  "description": "Shared NestJS module for Unique platform API integration",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "format": "biome format .",
    "format:fix": "biome format . --write",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@unique-ag/utils": "workspace:*",
    "bottleneck": "^2.19.5",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "remeda": "^2.32.0",
    "undici": "^7.18.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^10 || ^11",
    "@nestjs/core": "^10 || ^11",
    "@opentelemetry/api": "^1.9.0",
    "nestjs-otel": "^7"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/testing": "^11.1.6",
    "@opentelemetry/api": "^1.9.0",
    "@suites/di.nestjs": "^3.0.1",
    "@suites/doubles.vitest": "^3.0.1",
    "@suites/unit": "^3.0.1",
    "@swc/core": "^1.13.5",
    "@types/node": "^22.13.14",
    "nestjs-otel": "^7.0.1",
    "reflect-metadata": "^0.2.2",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  }
}
